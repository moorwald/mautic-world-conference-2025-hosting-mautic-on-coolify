# Mautic Configuration
MAUTIC_VERSION=5.2.8
MAUTIC_PORT=8080
MAUTIC_TRUSTED_HOSTS=localhost,yourdomain.com

# Database Configuration
MAUTIC_DB_NAME=mautic
